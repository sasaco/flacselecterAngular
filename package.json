{
  "name": "flac-selecter",
  "version": "3.0.1",
  "description": "調査・計測データに基づく対策工設計ツール",
  "author": "公益財団法人 鉄道総合技術研究所",
  "private": true,
  "main": "main.js",
  "scripts": {
    "postinstall": "npx electron-builder install-app-deps",
    "electron:build": "npm run electron:tsc && ng build --base-href ./",
    "electron:tsc": "tsc -p tsconfig.electron.json",
    "start": "npm run electron:tsc && npm run start:web",
    "electron:serve": "wait-on tcp:4200 && npx electron . --serve",
    "build": "npm run build:prod && npx electron-builder build",
    "build:prod": "npm run electron:build -- -c production",
    "start:web": "ng serve -c dev",
    "build:web": "ng build -c production --base-href \"/flacselecterAngular/\""
  },
  "dependencies": {
    "@angular/common": "18.2.12",
    "@angular/compiler": "18.2.12",
    "@angular/compiler-cli": "18.2.12",
    "@angular/core": "18.2.12",
    "@angular/forms": "18.2.12",
    "@angular/language-service": "18.2.12",
    "@angular/platform-browser": "18.2.12",
    "@angular/platform-browser-dynamic": "18.2.12",
    "@angular/router": "18.2.12",
    "rxjs": "~7.5.0",
    "tslint": "~6.1.3",
    "zone.js": "~0.14.10",
    "electron-reloader": "^1.2.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "18.2.12",
    "@angular/cli": "18.2.12",
    "@angular/compiler-cli": "18.2.12",
    "@types/jasmine": "2.8.7",
    "autoprefixer": "^10.4.20",
    "electron": "^30.0.1",
    "electron-builder": "^24.13.3",
    "electron-debug": "^3.2.0",
    "http-server": "^14.1.1",
    "jasmine-core": "~4.5.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.1.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "npm-run-all": "^4.1.5",
    "npx": "^10.2.2",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.4.5",
    "wait-on": "^7.2.0",
    "webdriver-manager": "^12.1.9"
  }
}
